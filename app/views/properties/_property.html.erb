<div id="<%= dom_id property %>">
  <p>
    <strong>Name:</strong>
    <%= property.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= property.description %>
  </p>

  <p>
    <strong>Adress:</strong>
    <%= property.adress %>
  </p>

  <% property.photos.each do |photo| %>
    <div style='width: 300px; height: 300px;'>
    <%= image_tag(photo, style:' width: 100%; height: auto;') %>
    </div>
  <% end %>

</div>
